{
    "builds": [
        {
            "src": "server.js",
            "use": "@now/node-server"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "api/**/*.js": {}
    },
    "scripts": {
        "build": "npm install",
        "start": "npm run prestart"
    }
}